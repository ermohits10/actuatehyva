<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Ced_Onbuy::Onbuy" title="OnBuy" sortOrder="50">
                    <resource id="Ced_Onbuy::onbuy_products" title="Products">
                        <resource id="Ced_Onbuy::profiles" title="Profile Manager"></resource>
                        <resource id="Ced_Onbuy::products" title="Product Manager"></resource>
                    </resource>
                    <resource id="Ced_Onbuy::onbuy_orders" title="Orders">
                        <resource id="Ced_Onbuy::view_onbuy_orders" title="Manage Orders"></resource>
                    </resource>
                    <resource id="Ced_Onbuy::onbuy_account" title="OnBuy Configuration">
                        <resource id="Ced_Onbuy::accounts" title="Manage Accounts"></resource>
                        <resource id="Ced_Onbuy::configuration" title="Configuration"></resource>
                    </resource>
                    <resource id="Ced_Onbuy::developer" title="Developer">
                        <resource id="Ced_Onbuy::crons" title="Cron Logs"></resource>
                        <resource id="Ced_Onbuy::feeds" title="Product Feeds"></resource>
                        <resource id="Ced_Integrator::activity_logs" title="Activity Logs"></resource>
                    </resource>
                    <resource id="Ced_Onbuy::extra" title="Extras">
                        <resource id="Ced_Onbuy::knowledge_base" title="Help And Support"></resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Ced_Onbuy::system_config" title="OnBuy Configuration"></resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>

